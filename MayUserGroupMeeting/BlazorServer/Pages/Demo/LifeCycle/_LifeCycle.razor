<h3>Child Component for Page/Component Life</h3>

@foreach (var item in EventType)
{
    @item
    <hr/>
}

@code {
    [Parameter]
    public int CountValue { get; set; }

    List<string> EventType = new List<string>();

    protected override void OnInitialized()
    {
        EventType.Add("Child Component - OnInitialized was called");
    }

    protected override void OnParametersSet()
    {
        EventType.Add("Child Component - OnParametersSet was called");
    }
}